{
  "name": "osm-parking-tag-updater",
  "private": true,
  "version": "0.0.23",
  "type": "module",
  "scripts": {
    "start": "vite --host 127.0.0.1",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "vitest watch",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "check": "npm run type-check && npm run lint && npm run format && vitest run",
    "deploy": "chmod +x ./deploy.sh && ./deploy.sh",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "@tanstack/react-location": "^3.7.4",
    "@tanstack/react-location-devtools": "^3.4.4",
    "@tanstack/react-query": "^4.19.1",
    "@tanstack/react-query-devtools": "^4.19.1",
    "axios": "^1.2.1",
    "clsx": "^1.2.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/ui": "^0.25.7",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "install": "^0.13.0",
    "npm": "^9.2.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vite-plugin-svgr": "^2.3.0",
    "vitest": "^0.25.7"
  }
}
